def applyToEach(L, f):
    """
    Assumes L is a list, f a functions
    Mutates L by replacing each element,
    e, of L by f(e)
    """
    for i in range(len(L)):
        L[i] = f(L[i])

L = [1, -2, 3.4]

applyToEach(L, abs)
print(L)

applyToEach(L, int)
print(L)

applytoEach(L, fact)
print(L)

applyToEach(L, fib)
print(L)
